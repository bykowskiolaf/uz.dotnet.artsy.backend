{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Authentication": "Information"
    }
  },
  "Database": {
    "Host": "localhost",
    "Port": 5432,
    "Name": "artsy",
    "Username": "postgres",
    "Password": "password"
  },
  "Jwt": {
    "Key": "YOUR_SUPER_SECRET_COMPLEX_KEY_HERE_REPLACE_THIS_LONG_ENOUGH",
    "Issuer": "http://localhost:5127",
    "Audience": "http://localhost:5127/api",
    "DurationInMinutes": "15",
    "RefreshTokenTTLDays": "7",
    "MaxActiveSessionsPerUser": "2"
  },
  "CookieSettings": {
    "Secure": true,
    "SameSiteAccessToken": "Strict",
    "PathAccessToken": "/api",
    "SameSiteRefreshToken": "Strict",
    "PathRefreshToken": "/api/auth/refresh"
  },
  "ArtsyApiSettings": {
    "BaseUrl": "https://api.artsy.net/api",
    "TokenUrl": "https://api.artsy.net/api/tokens/xapp_token",
    "ClientId": "YOUR_CLIENT_ID",
    "ClientSecret": "YOUR_CLIENT_SECRET",
  },
  "AllowedHosts": "*"
}
